  "scripts": {
    "start": "vite",
    "build": "NODE_ENV=production vite build",
    "build:local": "SENTRY_RELEASE=' ' yarn build",
    "build:analyze": "BUNDLE_ANALYZE=1 yarn build:local",
    "test": "DEBUG_PRINT_LIMIT=100000 vitest",
    "test:e2e": "playwright test --project=chromium --ui",
    "test:sql": "vitest --config vitest.sql.config.js",
    "test:e2e:all": "playwright test",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "eslint --ignore-path .gitignore --fix .",
    "playwright": "playwright",
    "preview": "SENTRY_RELEASE=' ' vite preview --port 3000",
    "typecheck": "tsc",
    "gen:api": "openapi-typescript",
    "gen:types": "bin/generate-types",
    "gen:mz-doc-urls": "ts-node-esm sitemap-to-json/index.ts"
