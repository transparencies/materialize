// Vitest Snapshot v1, https://vitest.dev/guide/snapshot.html

exports[`buildClusterReplicasQuery > builds a valid query 1`] = `
{
  "parameters": [
    "u1",
  ],
  "sql": "select "cr"."name", "cr"."size", ("crs"."memory_bytes" * "crs"."processes")::text as "memoryBytes", ("crs"."disk_bytes" * "crs"."processes")::text as "diskBytes", crhm.heap_limit::text as "heapLimit" from "mz_cluster_replicas" as "cr" inner join "mz_cluster_replica_sizes" as "crs" on "cr"."size" = "crs"."size" left join (select "crm"."replica_id", MAX(crm.heap_limit) as "heap_limit", MAX(crm.heap_bytes) as "heap_bytes" from "mz_cluster_replica_metrics" as "crm" group by "crm"."replica_id") as "crhm" on "crhm"."replica_id" = "cr"."id" where "cr"."cluster_id" = $1 order by "heapLimit"",
}
`;
